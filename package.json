{
  "name": "ludoteca",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@popperjs/core": "^2.9.1",
    "axios": "^0.x",
    "babel-eslint": "^10.x",
    "bootstrap": "^4.x",
    "bootstrap-icons": "^1.4.0",
    "bootstrap-vue": "^2.x",
    "feather-icons": "^4.28.0",
    "js-cookie": "^2.x",
    "node-sass": "^5.0.0",
    "vue": "^2.x",
    "vue-currency-input": "^1.22.6",
    "vue-router": "^3.x",
    "vue-toastification": "^1.x",
    "vuelidate": "^0.x",
    "vuex": "^3.x",
    "xml2json-light": "^1.x",
    "yarn": "^1.x"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "4.5.12",
    "@vue/cli-plugin-eslint": "4.5.12",
    "@vue/cli-service": "4.5.12",
    "eslint": "^7.22.0",
    "eslint-plugin-prettier-vue": "^2.1.1",
    "eslint-plugin-vue": "^7.8.0",
    "husky": "^6.0.0",
    "lint-staged": "^10.5.4",
    "sass-loader": "^10.x",
    "vue-template-compiler": "^2.x"
  },
  "babel": {
    "presets": [
      "@vue/app"
    ]
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "semi": [
        2,
        "never"
      ]
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix"
  }
}
